<div {{ attributes.addClass('block-liste-reseaux', 'container', 'block') }}>
    {{ title_prefix }}
    {% if label %}
        <div class="custom-title container"><h2{{ title_attributes.addClass('container') }}>{{ label }}</h2></div>
    {% endif %}
    {{ title_suffix }}
    {% block content %}
        {{ content }}
    {% endblock %}
</div>
